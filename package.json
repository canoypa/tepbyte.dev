{
  "name": "tepbyte.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run prepare && next build",
    "start": "next start",
    "lint": "pnpm run lint:next & pnpm run lint:ts",
    "lint:next": "next lint",
    "lint:ts": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "prepare": "panda codegen"
